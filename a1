loadstring(game:HttpGet('https://raw.githubusercontent.com/danchossssss/test-a1/main/ui'))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

local Window = Fluent:CreateWindow({
    Title = "a1 " ,
    SubTitle = "dev build 0.3.1",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true, -- The blur may be detectable, setting this to false disables blur entirely
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl -- Used when theres no MinimizeKeybind
})
local Tabs = {
    Scripts = Window:AddTab({ Title = "Scripts", Icon = "axe" }),
    Player = Window:AddTab({ Title = "Player", Icon = "users" }),
    Settings = Window:AddTab({ Title = "Settings", Icon = "settings" })
}

local Options = Fluent.Options


    Tabs.Scripts:AddButton({
        Title = "IY",
        Description = "Universal",
        Callback = function()
            Window:Dialog({
                Title = "IY",
                Content = "Execute script?",
                Buttons = {
                    {
                        Title = "Confirm",
                        Callback = function()
                            loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
                        end
                    },
                    {
                        Title = "Cancel",
                        Callback = function()
                            print("Cancelled the dialog.")
                        end
                    }
                }
            })
        end
    })


    Tabs.Scripts:AddButton({
        Title = "Dex",
        Description = "Universal",
        Callback = function()
            Window:Dialog({
                Title = "Dex",
                Content = "Execute script?",
                Buttons = {
                    {
                        Title = "Confirm",
                        Callback = function()
                            loadstring(game:HttpGet("https://raw.githubusercontent.com/Babyhamsta/RBLX_Scripts/main/Universal/BypassedDarkDexV3.lua", true))()
                        end
                    },
                    {
                        Title = "Cancel",
                        Callback = function()
                            print("Cancelled the dialog.")
                        end
                    }
                }
            })
        end
    })

    Tabs.Scripts:AddButton({
        Title = "Porry's Script",
        Description = "Ro ghoul",
        Callback = function()
            Window:Dialog({
                Title = "Porry's Script",
                Content = "Execute script?",
                Buttons = {
                    {
                        Title = "Confirm",
                        Callback = function()
                            loadstring(game:HttpGet("https://raw.githubusercontent.com/PorryDepTrai/exploit/main/DashBoostSolora.lua"))()
                        end
                    },
                    {
                        Title = "Cancel",
                        Callback = function()
                            print("Cancelled the dialog.")
                        end
                    }
                }
            })
        end
    })


    local Dropdown = Tabs.Player:AddDropdown("Dropdown", {
        Title = "Speed",
        Values = {"5", "Normal [16]", "25", "50"},
        Multi = false,
        Default = 1,
    })

    Dropdown:SetValue("Normal [16]")

Dropdown:OnChanged(function(Value)
    if Value == "5" then
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 5
    end
    if Value == "Normal [16]" then
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
    end
    if Value == "25" then
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 25
    end
    if Value == "50" then
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 50
    end
end)









InterfaceManager:SetLibrary(Fluent)

InterfaceManager:SetFolder("FluentScriptHub")


InterfaceManager:BuildInterfaceSection(Tabs.Settings)


Window:SelectTab(1)

Fluent:Notify({
    Title = "Fluent",
    Content = "The script has been loaded.",
    Duration = 8
})
